{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "builds": [
    {
      "src": "dist/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
